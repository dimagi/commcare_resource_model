usage:
  users:
    model: 'date_range_value'
    ranges:
      - ['20180101', '20190101', 100000]
      - ['20190101', '20200101', 200000]
      - ['20200101', '20210101', 300000]
  forms:
    model: 'cumulative'
    items_per_user: 600
  cases:
    model: 'cumulative'
    items_per_user: 200
  case_transactions:
    model: 'cumulative'
    items_per_user: 1600
  task_cases:
    model: 'limited_lifespan'
    items_per_user: 100
    lifespan: 1
#  person_cases:
#    model: 'cumulative'
#    items_per_user:
  task_ledgers:
    model: 'derived_factor'
    dependant_field: 'task_cases'
    factor: 5
  task_ledger_updates:
    model: 'derived_factor'
    dependant_field: 'task_cases'
    factor: 4
  synclogs:
    model: 'derived_factor'
    dependant_field: 'users'
    factor: 10
  device_logs:
    model: 'limited_lifespan'
    items_per_user: 1600
    lifespan: 1
  images:
    model: 'limited_lifespan'
    items_per_user: 33
    lifespan: 3
  case_indices:
    model: 'derived_factor'
    dependant_field: 'cases_cum'
    factor: 0.92
  kafak_changes:
    model: 'derived_sum'
    dependant_fields: ['forms', 'cases', 'case_transactions', 'task_ledger_updates']

storage:
  sql_primary:
    - field: 'forms_cum'
      bytes: 1200
    - field: 'cases_cum'
      bytes: 1800
    - field: 'case_indices'
      bytes: 380
    - field: 'case_transactions_cum'
      bytes: 515
    - field: 'task_ledgers'
      bytes: 450
    - field: 'task_ledger_updates'
      bytes: 420
